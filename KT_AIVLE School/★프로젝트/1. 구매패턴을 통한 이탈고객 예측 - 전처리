   "source": [
    "#[문제71] 전처리된 데이터프레임 'cust_churn'을 CSV 파일로 저장합니다."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 459,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 파일 : 'cust_churn.csv'\n",
    "# to_csv 함수 활용\n",
    "cust_churn.to_csv('cust_churn.csv', index=False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": []
  },
