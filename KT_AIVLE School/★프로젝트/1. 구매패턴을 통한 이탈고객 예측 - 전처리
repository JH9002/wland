    "# groupby, sum 함수 활용\n",
    "cate_amt2=cate_amt.groupby(by='Category',as_index=False)[['Amt']].sum()\n",
    "cate_amt2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
