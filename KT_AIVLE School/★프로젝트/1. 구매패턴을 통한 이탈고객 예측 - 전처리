    "# 연도는 'Year', 월은 'Month'로 변수를 추가\n",
    "# 'OrderDate' 컬럼에서 dt.year, dt.month 함수 활용\n",
    "sales['Year'] = sales['OrderDate'].dt.year\n",
    "sales['Month'] = sales['OrderDate'].dt.month\n",
    "sales.head()"
   ]
  },
