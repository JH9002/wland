    "# merge 함수 활용\n",
    "top_amt=pd.merge(sales, products, on='ProductID', how='outer')\n",
    "top_amt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": []
